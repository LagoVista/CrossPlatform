             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
            LeftMenu="Cancel"
            RightMenu="Save"
             x:Class="SeaWolf.Views.ConfigurationView">
