<?xml version="1.0" encoding="utf-8" ?>
<pge:LagoVistaContentPage
    xmlns:pge="clr-namespace:LagoVista.XPlat.Core;assembly=LagoVista.XPlat.Core"
     xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LagoVista.XPlat.Sample.Views.SettingsView">
    <pge:LagoVistaContentPage.MainContent >
        <StackLayout>
            <Button Text="Add" Command="{Binding AddSettingCommand}" />
            <Button Text="Has" Command="{Binding HasSettingCommand}" />
            <Button Text="Get" Command="{Binding GetSettingCommand}" />
            <Button Text="Remove" Command="{Binding RemoveSettingCommand}" />
        </StackLayout>
    </pge:LagoVistaContentPage.MainContent>
</pge:LagoVistaContentPage>