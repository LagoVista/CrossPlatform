<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="LinkColor">#D48D17</Color>

    <Color x:Key="SystemAccentColor">Purple</Color>

    <SolidColorBrush x:Key="SystemControlBackgroundAltHighBrush" Color="{ThemeResource EditControlBackground}"></SolidColorBrush>
    <SolidColorBrush x:Key="SystemControlBackgroundBaseLowBrush" Color="Green"></SolidColorBrush>
    
    <SolidColorBrush x:Key="SystemControlDisabledBaseLowBrush" ></SolidColorBrush>
    <SolidColorBrush x:Key="SystemControlDisabledTransparentBrush"></SolidColorBrush>
    <SolidColorBrush x:Key="SystemControlDisabledChromeDisabledLowBrush"></SolidColorBrush>

    <SolidColorBrush x:Key="SystemControlForegroundBaseHighBrush" Color="{ThemeResource EditControlText}"></SolidColorBrush>
    <SolidColorBrush x:Key="SystemControlForegroundChromeDisabledLowBrush" Color="{ThemeResource EditControlTextDisabled}"></SolidColorBrush>
    <SolidColorBrush x:Key="SystemColorWindowTextColor" Color="{ThemeResource EditControlTextActive}"></SolidColorBrush>
   
    
    <SolidColorBrush x:Key="SystemControlHighlightAccentBrush" Color="{StaticResource LinkColor}"></SolidColorBrush>
    <SolidColorBrush x:Key="SystemControlHighlightAltChromeWhiteBrush">White</SolidColorBrush>
    <SolidColorBrush x:Key="SystemControlHighlightChromeAltLowBrush">Tan</SolidColorBrush>

    <!-- Placeholder Not Focused -->
    <SolidColorBrush x:Key="SystemControlPageTextBaseMediumBrush">Orange</SolidColorBrush>
    <!-- PlaceHolder Focus -->
    <SolidColorBrush x:Key="SystemControlPageTextChromeBlackMediumLowBrush">Blue</SolidColorBrush>



